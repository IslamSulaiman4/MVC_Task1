@model List<Employee>

<h1>All Employees</h1>

<table class="table">
	<thead>
		<tr>
			<th scope="col">ID</th>
			<th scope="col">Name</th>
			<th scope="col">Title</th>
			<th scope="col">Age</th>
		</tr>
	</thead>

	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.title</td>
				<td>@item.Age</td>
			</tr>
		}
	</tbody>
</table>